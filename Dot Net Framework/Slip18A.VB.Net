Public Class Form1

    Private Sub Button1_Click(ByVal sender As System.Object, ByVal e As System.EventArgs) Handles Button1.Click
        Dim no As Integer
        Dim i As Integer
        no = CInt(InputBox("Enter Number"))
        For i = 1 To 10
            ListBox1.Items.Add(no * i)
        Next


    End Sub
End Class
